@page "/"
@using BlazorDynamicForm.Panel
@using static BlazorDynamicFormApp.Pages.Weather

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<div class="row">
    <div class="col-6">
        <DynamicPanelComponent Model="Model" />
    </div>
</div>

@code {
    private WeatherForecast Model = new WeatherForecast
    {
        TemperatureC = 32,
        Date = DateTime.Now,
        Summary = "Quite hot"
    };
}
